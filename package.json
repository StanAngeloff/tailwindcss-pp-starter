{
  "name": "@stanangeloff/tailwindcss-pp-starter",
  "private": true,
  "version": "0.1.0",
  "author": "PSP UK Group Ltd. <hello@psp-webtech.co.uk>",
  "scripts": {
    "start": "run-p -sr *:watch",
    "build": "run-p -c *:build",
    "fiveserver:watch": "five-server",
    "tailwind:watch": "cross-env NODE_ENV=development tailwindcss -i src/styles.css -o public/styles.css --watch",
    "tailwind:build": "cross-env NODE_ENV=production tailwindcss -i src/styles.css -o public/styles.css"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.4.1",
    "@tailwindcss/typography": "^0.5.2",
    "postcss-import": "^14.1.0",
    "postcss-preset-env": "^7.7.0",
    "postcss-sort-media-queries": "^4.2.1",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "five-server": "^0.1.4",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">=v16.15",
    "npm": "^8.5"
  }
}
